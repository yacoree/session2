<Window x:Class="Import.Windows.CostChangeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Import.Windows"
        mc:Ignorable="d"
        Title="CostChangeWindow" Height="140" Width="180">
    <Grid>
        <TextBox x:Name="CostChange_TB" HorizontalAlignment="Center" Margin="0,44,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="CostChange_Btn" Content="Change" HorizontalAlignment="Center" Margin="0,0,0,20" Height="20" VerticalAlignment="Bottom" Click="CostChange_Btn_Click"/>

    </Grid>
</Window>
